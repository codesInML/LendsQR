    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.4.0",
    "express-validator": "^6.14.2",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.2.0",
    "mysql": "^2.18.1",
    "nanoid": "^3.1.30",
    "sqlite3": "^5.0.11",
    "winston": "^3.8.0",
    "xss-clean": "^0.1.1"
  },
