  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll --no-cache",
    "build": "tsc -p .",
    "dev": "nodemon --legacy-watch --watch './src/**/*.ts' --exec 'ts-node' ./src/index.ts",
    "start": "node ./build/index.js",
    "knex:migrate": "npx knex migrate:latest --knexfile ./src/knexfile.ts",
